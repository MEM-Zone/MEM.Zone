//---------------| SQL ProductID class |---------------
#Pragma DeleteClass("SQL_ProductID", NOFAIL)

//-- Create custom WMI class
//-  Class qualifiers
    [
        Description("The SQL_ProductID class contains the obfuscated SQL license key. You can use this information for SQL reporting."): ToInstance
    ]

//-  Class properties
Class SQL_ProductID
{
    [key]
    string  Release;
    string  ProductID;
    boolean DigitalProductID[];
};

/- SQL 2025 class instance
[DYNPROPS]
Instance of SQL_ProductID
{
    Release = "2025";
    [
        PropertyContext("Local|HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Microsoft SQL Server\\170\\Tools\\ClientSetup|ProductID"),
        Dynamic,
        Provider("RegPropProv")
    ]
    ProductID;
    [
        PropertyContext("Local|HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Microsoft SQL Server\\170\\Tools\\ClientSetup|DigitalProductID"),
        Dynamic,
        Provider("RegPropProv")
    ]
    DigitalProductID;
};

